# SOLUTION
print("WELCOME TO GRADING SYSTEM")

mark = int(input("Enter mark attained"))
if  91<=mark<=100:
   print("A+")
elif 80<=mark<=90:
   print("A")
elif 70<=mark<=79:
   print("B")
elif 60<=mark<=69:
   print("C")
elif 50<=mark<=59:
   print("D")
elif 35<=mark<=49:
   print("E")
elif  0<=mark<=34:
   print("F")
else:
   print("Invalid")